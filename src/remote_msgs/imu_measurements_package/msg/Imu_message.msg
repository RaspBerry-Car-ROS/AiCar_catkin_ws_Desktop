#This message sends IMU measurements
float32 acceleration_x_gF #X axis acceleration in g-Force units
float32 acceleration_y_gF #Y axis acceleration in g-Force units
float32 acceleration_z_gF #Z axis acceleration in g-Force units

float32 rotation_x_degSec #X axis rotation in degrees/sec units
float32 rotation_y_degSec #Y axis rotation in degrees/sec units
float32 rotation_z_degSec #Z axis rotation in degrees/sec units

float32 compass_x_gauss #X axis direction in gauss units
float32 compass_y_gauss #Y axis direction in gauss units
float32 compass_z_gauss #Z axis direction in gauss units